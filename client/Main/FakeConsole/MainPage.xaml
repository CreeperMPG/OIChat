<Page x:Class="OIChatRoomClient.Main.FakeConsole.MainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:OIChatRoomClient.Main.FakeConsole"
      mc:Ignorable="d" 
      xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MainPage">

    <Grid Background="Black">
        <ui:TitleBar Height="30" Title="C:\Users\Default\Desktop\Program.exe" Name="appTitle">
            <ui:TitleBar.Icon>
                <ui:ImageIcon Source="pack://application:,,,/Resources/Icons/system.png" />
            </ui:TitleBar.Icon>
        </ui:TitleBar>
        <TextBlock Margin="5, 35, 5, 5" x:Name="fakeConsoleContent" Foreground="White"/>

        <Rectangle x:Name="pausePoint" Fill="White" Width="13" Height="5" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="140, 195, 0, 0"/>

    </Grid>
</Page>
